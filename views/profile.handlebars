<script src="/js/add-post.js"></script>
<script src="/js/add-code.js"></script>
<script src="/js/delete-post.js"></script>
<script src="/js/like.js"></script>
<script src="/js/dislike.js"></script>
<script src="/js/add-comment.js"></script>
